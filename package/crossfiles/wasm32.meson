[binaries]
c = 'emcc'
cpp = 'em++'
ar = 'emar'
strip = 'emstrip'
exe_wrapper = ['node']

[built-in options]
c_link_args = ['-sEXPORT_ALL=1']
cpp_link_args = ['-sEXPORT_ALL=1']

[properties]
needs_exe_wrapper = true

[host_machine]
system = 'emscripten'
cpu_family = 'wasm32'
cpu = 'wasm32'
endian = 'little'
